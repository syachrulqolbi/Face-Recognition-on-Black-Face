# --- Database ---
DB_DSN=postgresql://postgres:12345678@pgvec:5432/mydb
DB_SCHEMA=public
DB_TABLE=faces
# PG_DSN=...
# POSTGRES_SCHEMA=...
# POSTGRES_TABLE=...

# --- Model / paths ---
ARCFACE_ONNX=models/glintr100.onnx
QUERY_ROOT=data/query
REPS_DIR=reps

# --- Outputs (optional) ---
OUT_FAISS=gallery_flat.faiss
OUT_NPY=gallery_full.npy
OUT_TXT=gallery_ids.txt

# --- Runtime (optional) ---
TOPK=5
BATCH=64
SAVE_REPS=false
PREPROC_PIPELINE=grayworld,melanin,msr,tantriggs

# --- Inference (ONNX Runtime) â€” all OPTIONAL ---
# Choose provider: auto | cpu | cuda | dml  (auto = prefer GPU if available)
ORT_PROVIDER=auto
# Force CPU even if GPU is available (true/false)
ORT_FORCE_CPU=false
# Threading: 0 = auto (use all cores). Set >0 to pin.
ORT_THREADS=0
# You can also pin BLAS threads (optional; leave empty to let code auto-set)
# OMP_NUM_THREADS=8
# OPENBLAS_NUM_THREADS=8
